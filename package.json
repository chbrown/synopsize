{
  "name": "synopsize",
  "version": "0.2.0",
  "description": "Print out a synopsis of values from a stream of newline-delimited JSON objects",
  "keywords": [
    "json",
    "describe",
    "summarize",
    "synopsis"
  ],
  "homepage": "https://github.com/chbrown/synopsize",
  "repository": "github:chbrown/synopsize",
  "author": "Christopher Brown <io@henrian.com>",
  "license": "MIT",
  "dependencies": {
    "optimist": "^0.6.1",
    "streaming": "^1.1.1",
    "@chbrown/sv": "^0.4.4",
    "tarry": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/optimist": "^0.0.33",
    "typescript": "^5.5.4"
  },
  "bin": {
    "synopsize": "bin/synopsize.js"
  },
  "scripts": {
    "prepare": "tsc --build && chmod +x bin/synopsize.js",
    "clean": "tsc --build --clean"
  }
}
